<%= form_with url: "/applications", method: :post, local: true do |form| %>
  <%= form.label :name%><br/>
  <%= form.text_field :name %><br/>
  <%= form.label :address %><br/>
  <%= form.text_field :address %><br/>
  <%= form.label :city %><br/>
  <%= form.text_field :city %><br/>
  <%= form.label :state %><br/>
  <%= form.text_field :state %><br/>
  <%= form.label :zip %><br/>
  <%= form.text_field :zip %><br/>
  <%= form.submit "Submit" %>
<% end %>
